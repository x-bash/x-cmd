
# emit event:
#   file created
#   directory created
#   file deleted

___x_cmd_fmon(){
    local subcmd="$1"
    case "$subcmd" in
        ""|help)    ;;
    esac
}

___x_cmd_fmon_check(){

}

___x_cmd_until_is_int(){
    if [ "$1" -gt 0 ] || [ "$1" -le 0 ]; then
        return 0
    fi 2>/dev/null
}


