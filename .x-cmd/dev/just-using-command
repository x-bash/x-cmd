
        eval)                                   shift;  ___x_cmd_eval "$@"      ;;
        readml)                                 shift;  ___x_cmd_readml "$@"    ;;
        writeml)                                shift;  ___x_cmd_writeml "$@"   ;;

        jar|java|npm|npx|ts|        arg1|args|conjoin|con|rmrf|repeat|     which|search_path|_x_cmd_advise_json|       ls|ll   |ssl    | abspath \
            | ps|du|sqlite|cat|bw | 7z|7zd )
                                                shift;  "___x_cmd_$subcmd" "$@" ;;
